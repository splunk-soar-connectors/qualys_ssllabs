                "publish": {
                    "data_type": "string",
                    "order": 1,
                    "description": "Publish report publicly ([off]/on)",
                    "default": "off",
                    "value_list": [
                        "off",
                        "on"
                    ],
                    "required": false
                },
                "start_new": {
                    "data_type": "string",
                    "order": 2,
                    "description": "Start a new assessment ([off]/on)",
                    "default": "off",
                    "value_list": [
                        "off",
                        "on"
                    ],
                    "required": false
                },
                "max_age": {
                    "data_type": "numeric",
                    "order": 3,
                    "description": "Maximum age in hours of cached report",
                    "required": false
                },
                "return_all_data": {
                    "data_type": "string",
                    "order": 4,
                    "default": "off",
                    "value_list": [
                        "off",
                        "on"
                    ],
                    "description": "Return full information ([off]/on)",
                    "required": false
                },
                "ignore_mismatch": {
                    "data_type": "string",
                    "order": 5,
